{
  "name": "get-random-values",
  "version": "0.1.0",
  "description": "`crypto.getRandomBytes` polyfill for node.",
  "main": "index.cjs",
  "module": "index.js",
  "type": "module",
  "files": [
    "*.js",
    "*.cjs"
  ],
  "exports": {
    "./phonyfill": {
      "import": "./phonyfill.js",
      "require": "./phonyfill.cjs"
    },
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "scripts": {
    "build": "node scripts/build.js index.js phonyfill.js",
    "prepack": "npm run build"
  },
  "keywords": [
    "node",
    "crypto",
    "getrandombytes",
    "random",
    "bytes",
    "webcrypto",
    "web-cryptography",
    "web-cryptography-api"
  ],
  "author": "Florian Klampfer <mail@qwtel.com> (https://qwtel.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qwtel/node-get-random-values.git"
  },
  "bugs": {
    "url": "https://github.com/qwtel/node-get-random-values/issues"
  },
  "homepage": "https://github.com/qwtel/node-get-random-values#readme"
}
